{
  "name": "partinator",
  "version": "0.0.1",
  "description": "Backend System for Fastener E-Commerce Catalog",
  "main": "src/server.ts",
  "type": "module",
  "engine": "node >=22.2.0",
  "scripts": {
    "fullclean": "rm -rf ./node_modules ./coverage && yarn run clean",
    "clean": "rm -rf ./build/",
    "build": "tsc",
    "start": "yarn run build && npx tsx src/app.ts",
    "dev": "export NODE_ENV=local && yarn run build && npx tsx watch src/app.ts",
    "test": "export NODE_ENV=test && jest --no-cache --detectOpenHandles",
    "test:dev": "yarn run clean && yarn run build && yarn run test",
    "test:prepush": "yarn run fullclean && yarn install && yarn run build && yarn run test",
    "test:services": "export NODE_ENV=test && yarn build && export TEST_TYPE=services && jest",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "Luke Elliot",
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cross-fetch": "^4.0.0",
    "common-es": "^1.0.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "express": "^4.17.1",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "prettier": "^3.1.0",
    "pretty-format": "^29.7.0",
    "sequelize": "^6.37.1",
    "svix": "^1.3.0",
    "tsx": "^4.11.2",
    "umzug": "^3.7.0",
    "validator": "^13.6.0",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zipcodes": "^8.0.0"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0",
    "supertest": "^6.0.2",
    "@types/supertest": "^6.0.2",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@ngneat/falso": "^7.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/jest-when": "^3.5.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.2.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@types/eslint": "^9.6.1",
    "babel-jest": "^29.7.0",
    "cliui": "^8.0.1",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.6.0",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.2.2"
  }
}

