{
  "name": "partinator",
  "version": "0.0.1",
  "description": "Backend System for Fastener E-Commerce Catalog",
  "main": "build/server.js",
  "type": "commonjs",
  "scripts": {
    "clean": "rm -rf ./build/",
    "build": "tsc",
    "start": "yarn run build && npx tsx src/server.ts",
    "start:ui": "cd src/ui/ && yarn start",
    "start:all": "concurrently \"yarn start\" \"yarn start:ui\"",
    "dev": "yarn run build && npx tsx watch src/server.ts",
    "test": "yarn run reset && yarn run build && NODE_OPTIONS=--experimental-import-meta-resolve && jest --config jest.config.js --runInBand",
    "test:prepush": "NODE_ENV=test && yarn run clean:full && yarn install && yarn run build && yarn run test && yarn run start",
    "lint": "eslint . --config config/eslintrc.js --ext .ts",
    "migrate:generate": "npx sequelize-cli migration:generate --name",
    "migrate": "npx ts-node -r tsconfig-paths/register ./node_modules/.bin/sequelize-cli db:migrate --config=config/sequelize-cli-config.js",
    "reset": "rimraf node_modules && yarn cache clean && yarn install"
  },
  "keywords": [],
  "author": "Luke Elliot",
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "common-es": "^1.0.1",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "express-validator": "^7.2.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.1",
    "prettier": "^3.1.0",
    "pretty-format": "^29.7.0",
    "sequelize": "^6.37.4",
    "svix": "^1.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.11.2",
    "umzug": "^3.7.0",
    "validator": "^13.6.0",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zipcodes": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@jest/globals": "^29.7.0",
    "@ngneat/falso": "^7.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^9.6.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.12",
    "@types/jest-when": "^3.5.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.2.0",
    "@types/sequelize": "^4.28.20",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^29.7.0",
    "cliui": "^8.0.1",
    "concurrently": "^9.0.1",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "npx": "^10.2.2",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.0.3",
    "supertest": "^6.0.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
